"""
Inheritance
"""
class Faculty:
    def __init__(self,f_name,department,f_id):
        self.f_name=f_name
        self.department=department
        self.f_id=f_id
    def print_info(self):
        print("faculty information",self.f_name,self.department,self.f_id)
ob=Faculty("pras","CSE",4436)
ob.print_info()

class Cse(Faculty):
    pass
ob=Cse("siri","CSE",4417)
ob.print_info()


class Cse(Faculty):
    pass
ob=Cse("siri","CSE",4417)
ob.print_info()
